<Page x:Class="TeleCompany.Pages.Registration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TeleCompany.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Registration">

    <Grid Background="Aquamarine" Height="450" VerticalAlignment="Center" HorizontalAlignment="Left" Width="800">
        <TextBlock TextAlignment="Center" FontSize="36" Margin="0,0,0,340">Auth</TextBlock>
        <TextBlock Margin="280,142,280,291">Login</TextBlock>
        <TextBox Margin="280,159,280,256"></TextBox>
        <PasswordBox Margin="280,208,280,208"></PasswordBox>
        <Button Margin="280,297,280,119">Register</Button>
        <ComboBox Margin="280,258,280,158"></ComboBox>
        <TextBlock Margin="280,191,280,242"><Run Text="Password"/></TextBlock>
        <TextBlock Margin="280,240,280,193"><Run Text="Role"/></TextBlock>
    </Grid>
</Page>
